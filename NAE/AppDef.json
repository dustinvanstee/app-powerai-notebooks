{
    "name": "PowerAI Notebooks",
    "description": "Jupyter Notebooks for various PowerAI ML/DL frameworks, including TensorFlow&trade;, Chainer, and Theano.",
    "author": "Nimbix, Inc.",
    "licensed": true,
    "classifications": [
        "Machine Learning/Deep Learning",
        "Machine Learning/Artificial Intelligence",
        "IBM POWER/Machine Learning/Deep Learning",
        "IBM POWER/Machine Learning/Artificial Intelligence"
    ],
    "machines": ["*"],
    "vault-types": [
        "FILE",
        "BLOCK",
        "BLOCK_ARRAY",
        "OBJECT"
    ],
    "commands": {
        "Chainer": {
            "path": "/usr/local/bin/nimbix_notebook",
            "interactive": true,
            "name": "Chainer",
            "description": "Launch a Jupyter Notebook to use Chainer directly in your browser.",
            "parameters": {
                "-s": {
                    "name": "-s",
                    "description": "-s",
                    "type": "CONST",
                    "value": "/opt/DL/tensorflow/bin/chainer-activate",
                    "positional": false,
                    "required": true
                },
                "-r": {
                    "name": "Requirements file",
                    "description": "Select a text file to use as a list of pip requirements (e.g. requirements.txt)",
                    "type": "FILE",
                    "positional": false,
                    "required": false,
                    "filter": "*.txt"
                }
            }
        },
        "TensorFlow": {
            "path": "/usr/local/bin/nimbix_notebook",
            "interactive": true,
            "name": "TensorFlow&trade;",
            "description": "Launch a Jupyter Notebook to use TensorFlow&trade; directly in your browser.",
            "parameters": {
                "-s": {
                    "name": "-s",
                    "description": "-s",
                    "type": "CONST",
                    "value": "/opt/DL/tensorflow/bin/tensorflow-activate",
                    "positional": false,
                    "required": true
                },
                "-r": {
                    "name": "Requirements file",
                    "description": "Select a text file to use as a list of pip requirements (e.g. requirements.txt)",
                    "type": "FILE",
                    "positional": false,
                    "required": false,
                    "filter": "*.txt"
                }
            }
        },
        "Theano": {
            "path": "/usr/local/bin/nimbix_notebook",
            "interactive": true,
            "name": "Theano",
            "description": "Launch a Jupyter Notebook to use Theano directly in your browser.",
            "parameters": {
                "-s": {
                    "name": "-s",
                    "description": "-s",
                    "type": "CONST",
                    "value": "/opt/DL/tensorflow/bin/theano-activate",
                    "positional": false,
                    "required": true
                },
                "-r": {
                    "name": "Requirements file",
                    "description": "Select a text file to use as a list of pip requirements (e.g. requirements.txt)",
                    "type": "FILE",
                    "positional": false,
                    "required": false,
                    "filter": "*.txt"
                }
            }
        }
    },
    "image": {
        "type": "image/png",
        "data": ""
    },
    "id": "power8-powerai_notebooks"
}
